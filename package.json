		"vue2-leaflet": "^2.7.1"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/eslint-parser": "^7.16.3",
		"@types/leaflet": "^1.7.6",
		"@vue/cli-plugin-babel": "^4.5.15",
		"@vue/cli-plugin-eslint": "^4.5.15",
		"@vue/cli-service": "^4.5.15",
		"@vue/eslint-config-prettier": "^6.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-json": "^3.1.0",
