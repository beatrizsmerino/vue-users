		"node-emoji": "^1.11.0",
		"svg-symbol-sprite-loader": "^4.0.0",
		"vue": "^2.6.14",
		"vue-router": "^3.5.3",
		"vue2-leaflet": "^2.7.1"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/eslint-parser": "^7.17.0",
