		"glob-parent": "https://registry.npmjs.org/glob-parent/-/glob-parent-5.1.2.tgz",
		"json-schema": "https://registry.npmjs.org/json-schema/-/json-schema-0.4.0.tgz",
		"node-forge": "https://registry.npmjs.org/node-forge/-/node-forge-1.3.0.tgz",
		"nth-check": "https://registry.npmjs.org/nth-check/-/nth-check-2.0.1.tgz",
